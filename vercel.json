{
  "projects": {
    "frontend": {
      "src": "frontend",
      "builds": [
        { "src": "frontend/package.json", "use": "@vercel/static-build" }
      ],
      "routes": [
        { "src": "/(.*)", "dest": "/frontend/$1" }
      ]
    },
    "backend": {
      "src": "backend",
      "builds": [
        { "src": "backend/package.json", "use": "@vercel/node" }
      ],
      "routes": [
        { "src": "/api/(.*)", "dest": "/backend/api/$1" }
      ]
    }
  }
}
